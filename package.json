{
	"name": "silverstripe",
	"displayName": "SilverStripe",
	"description": "Syntax highlighter for SilverStripe template files (.ss)",
	"icon": "images/icon.png",
	"version": "0.0.7",
	"publisher": "coxy2001",
	"author": "coxy2001",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/coxy2001/vscode-silverstripe/issues"
	},
	"engines": {
		"vscode": "^1.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/coxy2001/vscode-silverstripe.git"
	},
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"keywords": [
		"php",
		"silverstripe",
		"html",
		"template"
	],
	"contributes": {
		"languages": [
			{
				"id": "SilverStripe",
				"aliases": [
					"HTML(SilverStripe)",
					"SilverStripe"
				],
				"extensions": [
					".ss"
				],
				"configuration": "./silverstripe-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "SilverStripe",
				"scopeName": "text.html.ss",
				"path": "./syntax/silverstripe.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "SilverStripe",
				"path": "./snippets/silverstripe-ss.json"
			},
			{
				"language": "php",
				"path": "./snippets/silverstripe-php.json"
			}
		]
	}
}
